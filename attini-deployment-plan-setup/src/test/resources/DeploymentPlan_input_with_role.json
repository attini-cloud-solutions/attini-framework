{
  "accountId": "655047308345",
  "fragment": {
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
      "EnvironmentName": {
        "Type": "String",
        "Default": "dev"
      },
      "AttiniDistributionName": {
        "Type": "String"
      },
      "AttiniDistributionId": {
        "Type": "String"
      },
      "Test": {
        "Type": "String"
      },
      "Container": {
        "Type": "String"
      }
    },
    "Outputs": {
      "Container": {
        "Value": {
          "Ref": "Container"
        }
      },
      "Test": {
        "Value": {
          "Ref": "Test"
        }
      }
    },
    "Resources": {
      "MyDeploymentPlan": {
        "Type": "Attini::Deploy::DeploymentPlan",
        "Properties": {
          "RoleArn" : "arn:aws:iam::655047308345:role/attini/attini-action-lambda-service-role-eu-west-1",
          "DeploymentPlan": {
            "StartAt": "InitLambda",
            "States": {
              "InitLambda": {
                "Type": "Task",
                "Resource": "arn:aws:lambda:eu-west-1:855066048591:function:dev-lambda-test",
                "TimeoutSeconds": 300,
                "Next": "DeployCfn"
              },
              "DeployCfn": {
                "Type": "AttiniCfn",
                "Properties" : {
                  "Template": "lambda.yaml",
                  "StackName": "MyFirstLambda",
                  "RoleArn": {
                    "Fn::GetAtt": "LambdaServiceRole.Arn"
                  }
                },
                "Next": "InvokeLambda"
              },
              "InvokeLambda": {
                "Type": "AttiniLambdaInvoke",
                "Parameters": {
                  "FunctionName" : "my-function"
                },
                "Next" : "Vpc"
              },
              "Vpc": {
                "Type": "Task",
                "Resource": "arn:aws:lambda:eu-west-1:855066048591:function:dev-lambda-test",
                "TimeoutSeconds": 300,
                "Next": "SuccessState"
              },
              "SuccessState": {
                "Type": "Succeed"
              }
            }
          }
        }
      },
      "LambdaServiceRole": {
        "Type": "AWS::IAM::Role",
        "Properties": {
          "AssumeRolePolicyDocument": {
            "Statement": [
              {
                "Action": "sts:AssumeRole",
                "Effect": "Allow",
                "Principal": {
                  "Service": "cloudformation.amazonaws.com"
                }
              }
            ],
            "Version": "2012-10-17"
          },
          "ManagedPolicyArns": [
              "arn:aws:iam::aws:policy/AdministratorAccess"
          ]
        }
      }
    }
  },
  "transformId": "655047308345::AttiniDeploymentPlan",
